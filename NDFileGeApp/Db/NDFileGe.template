include "NDFile.template"
include "NDPluginBase.template"






record(longin, "$(P)$(R)NDFileGe_num_events_RBV")
{
    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))NDFileGe_num_events")
    field(SCAN, "I/O Intr")
}

    
