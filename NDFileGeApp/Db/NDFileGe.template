    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))NDFileGe_fnum")
    field(SCAN, "I/O Intr")
}

    
record(longin, "$(P)$(R)NDFileGe_printAttributes_RBV")
